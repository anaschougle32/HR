{
  "expo": {
    "scheme": "acme",
    "plugins": [
      "expo-router",
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ],
    "name": "jobhr",
    "slug": "jobhr",
    "newArchEnabled": true,
    "android": {
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "ios": {
      "infoPlist": {
        "NSDocumentPickerUsageDescription": "This app needs access to documents for resume uploads."
      }
    }
  }
}
